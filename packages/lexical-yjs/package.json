        "default": "./dist/LexicalYjs.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "development": "./dist/LexicalYjs.dev.js",
        "production": "./dist/LexicalYjs.prod.js",
        "default": "./dist/LexicalYjs.js"
      }

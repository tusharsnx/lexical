{
  "name": "@lexical/yjs",
  "description": "The library provides Yjs editor bindings for Lexical.",
  "keywords": [
    "react",
    "lexical",
    "editor",
    "rich-text",
    "colllaboration",
    "yjs",
    "crdt"
  ],
  "license": "MIT",
  "version": "0.29.0",
  "main": "./dist/LexicalYjs.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@lexical/offset": "0.29.0",
    "@lexical/selection": "0.29.0",
    "lexical": "0.29.0"
  },
  "peerDependencies": {
    "yjs": ">=13.5.22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/lexical",
